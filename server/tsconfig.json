{
  "compilerOptions": {
    "module": "ESNext",
    "esModuleInterop": true,
    "target": "ES2023",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist/",
 

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
  },
  "lib": ["es2015"]
}